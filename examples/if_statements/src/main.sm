import std::io;

func main(): i8 {
    const flag: bool = true;
    if flag == true {
        io::println("The flag is true");
    } else {
        io::println("The flag is false");
    }


    const x: i8 = if flag == true {
        5;
    } else {
        6;
    };
    io::println(x);

    const y: i8 = if flag == true -> 5 else 6;
    io::println(y);
    ret 0;
}